<van-search 
  class="search"
  value="{{ value }}" 
  shape="round"
  label="{{canteen}}"
  use-left-icon-slot="false"
  right-icon="search"
  placeholder="请输入搜索关键词" 
  bind:search="onSearch"
>
</van-search>
<!-- 商品栏 -->
<view wx:key="this" wx:for="{{goods}}">
<van-card
  price="{{ item.price }}"
  desc="{{ item.desc }}"
  title="{{ item.name }}"
  thumb="{{ item.imageURL }}"
>
</van-card>
</view>


