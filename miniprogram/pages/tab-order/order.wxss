.order{
  margin-bottom: 50px;
}
.search {
  border-bottom-style: solid;
  border-bottom-color: #353535;
  border-bottom-width: 1px;
}

.canteen-view {
    display: flex;
    align-items: center;
    margin-right: 5px;
}

.canteen-select {
    font-size: small;
    margin-right: 3px;
}

.card-title {
    font-size: medium;
    font-weight: bold;
}

.card-desc {
    font-size: small;
}

.header {
    margin: 90rpx 0 90rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}
 
.header image {
    width: 200rpx;
    height: 200rpx;
}
 
.content {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
}
 
.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}
 
.bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
}